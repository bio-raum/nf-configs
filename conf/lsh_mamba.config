
// Job parameters to set for this environment

params {
	max_cpus = 8
	max_memory = 30.GB
	reference_base = "/projects/references"
}

// Resource manager settings

executor {
  queueSize=5
}

// Process setting

process {
  executor = 'local'
}

conda {
  enabled = true
  use_mamba = true
  cacheDir = "/projects/conda_cache"
}

